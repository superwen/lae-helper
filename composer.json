{
    "name": "superwen/lae-helpers",
    "description": "Helpers extension for laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "helpers"],
    "homepage": "https://github.com/superwen/lae-helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "superwen",
            "email": "superwen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Superwen\\Admin\\Helpers\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Superwen\\Admin\\Helpers\\HelpersServiceProvider"
            ]
        }
    }
}